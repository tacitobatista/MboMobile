<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="MbOMobile.MainPage"
            xmlns:local="clr-namespace:MbOMobile" 
            xmlns:local1="clr-namespace:MbOMobile.Views">

    <FlyoutPage.Flyout>
        <local1:FlyoutMenuPage x:Name="flyout"/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:LoginPage/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    

</FlyoutPage>
